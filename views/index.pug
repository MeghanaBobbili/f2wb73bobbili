extends layout

block content
  h1= title
  p Welcome to Meghana Bobbili Render page #{title}
  button(type="button", class="btn btn-secondary",onclick="doBump()") Press Me
  br
  div#bump Bump the count 
  br
  a(href="/movie") movie Page
  script.
      var count=0;
      var bump=1;
      function doBump()
      {
      count=count+bump;
      bump=bump+1;
      print=document.getElementById("bump");
      print.innerHTML='Count '+count+' with bump '+bump;
      }